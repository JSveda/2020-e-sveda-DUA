<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>
<?scenebuilder-stylesheet ./src/main/java/com/main/css/SectionDesign.css?>

<VBox styleClass="box" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.main.AlgorithmBasics">
   <children>
      <HBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" styleClass="box" VBox.vgrow="ALWAYS">
         <children>
            <VBox HBox.hgrow="ALWAYS">
               <children>
                  <WebView fx:id="webView" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
                  <HBox alignment="CENTER_RIGHT" prefHeight="50.0" spacing="20.0" style="-fx-background-color: rgb(11, 20, 37);" VBox.vgrow="NEVER">
                     <children>
                        <Button fx:id="mainMenuButton" mnemonicParsing="false" onMouseClicked="#goToMainMenu" text="Hlavní menu" />
                        <Button fx:id="previousPageButton" mnemonicParsing="false" onMouseClicked="#goToPreviousPage" text="Předchozí stránka" />
                        <Button fx:id="nextPageButton" mnemonicParsing="false" onMouseClicked="#goToNextPage" text="Následující stránka" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="20.0" left="30.0" right="30.0" top="15.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
